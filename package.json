{
    "name": "ezs-lodex",
    "version": "1.0.0",
    "description": "ezs dedicated statements for Lodex",
    "directories": {
        "test": "test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Inist-CNRS/ezs-lodex.git"
    },
    "keywords": [
        "ezs"
    ],
    "author": "Nicolas Thouvenin <nthouvenin@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Inist-CNRS/ezs-lodex/issues"
    },
    "homepage": "https://github.com/Inist-CNRS/ezs-lodex#readme",
    "dependencies": {},
    "main": "./lib/index.js",
    "scripts": {
        "test": "istanbul test ./node_modules/.bin/_mocha -- -R spec",
