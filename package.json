        "pretest": "npm run build",
        "preversion": "npm run doc",
        "postversion": "git push && git push --tags"
    },
    "devDependencies": {
        "babel-cli": "^6.24.1",
        "babel-eslint": "^7.2.1",
        "babel-preset-env": "1.7.0",
        "babel-preset-stage-3": "^6.24.1",
        "documentation": "8.1.2",
        "eslint": "^3.19.0",
        "eslint-config-airbnb": "^14.1.0",
        "eslint-plugin-import": "^2.2.0",
        "expect": "23.1.0",
        "ezs": "6.0.0",
        "from": "^0.1.7",
        "istanbul": "^0.4.5",
        "mocha": "^3.5.3"
    },
    "peerDependencies": {
        "ezs": "*",
        "ezs-basics": "^3.6.2"
